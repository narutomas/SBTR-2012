<%= render :partial => 'duels/header' %>

<div>
  <h3><%= @contestant_assignment.contestant.name.upcase %></h3>
  <ol>
    <% @contestant_assignment.photos.each do |photo| %>
      <% if image_url = photo.image_url %>
        <li><%= link_to image_tag(image_url), duel_path(@duel) %></li>
      <% end %>
    <% end %>
  </ol>
</div>

<p class="footer">
  <span><%= link_to "BACK", duel_path(@duel) %></span>
</p>
